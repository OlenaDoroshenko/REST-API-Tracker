{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
      ],
      "plugins": [
        "prettier"
      ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "prefer-destructuring": ["error", {"object": false, "array": false}],
        // "quotes": ["error", "double"],
        "prettier/prettier": [
            "error",
            {},
            {
              "fileInfoOptions": {
                "withNodeModules": true
              } 
            }
          ],
          "endOfLine": "auto"
    }
}
